<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.entity.info.NihongoOwningInfo" table="nihongo_owning_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="user_id" type="java.lang.Long">
			<column name="user_id" not-null="true" />
		</property>
		<property name="pet_id" type="java.lang.Long">
			<column name="pet_id" not-null="true" />
		</property>
		<property name="current_level" type="java.lang.Integer">
			<column name="current_level" not-null="true" />
		</property>
		<property name="created" type="java.util.Date">
			<column name="created" not-null="true" />
		</property>
		<property name="updated" type="java.util.Date">
			<column name="updated" not-null="true" />
		</property>
	</class>
</hibernate-mapping>