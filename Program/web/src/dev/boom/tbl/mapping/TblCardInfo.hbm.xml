<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblCardInfo" table="card_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="player_id" type="java.lang.Long">
			<column name="player_id" not-null="true" />
		</property>
		<property name="card_index" type="java.lang.Integer">
			<column name="card_index" not-null="true" />
		</property>
		<property name="card_base_index" type="java.lang.Integer">
			<column name="card_base_index" not-null="true" />
		</property>
		<property name="deed" type="java.lang.Integer">
			<column name="deed" not-null="true" />
		</property>
		<property name="hp" type="java.lang.Short">
			<column name="hp" not-null="true" />
		</property>
		<property name="atk" type="java.lang.Integer">
			<column name="atk" not-null="true" />
		</property>
		<property name="def" type="java.lang.Integer">
			<column name="def" not-null="true" />
		</property>
		<property name="spd" type="java.lang.Integer">
			<column name="spd" not-null="true" />
		</property>
		<property name="vir" type="java.lang.Integer">
			<column name="vir" not-null="true" />
		</property>
		<property name="stg" type="java.lang.Integer">
			<column name="stg" not-null="true" />
		</property>
		<property name="atk_level" type="java.lang.Short">
			<column name="atk_level" not-null="true" />
		</property>
		<property name="def_level" type="java.lang.Short">
			<column name="def_level" not-null="true" />
		</property>
		<property name="spd_level" type="java.lang.Short">
			<column name="spd_level" not-null="true" />
		</property>
		<property name="vir_level" type="java.lang.Short">
			<column name="vir_level" not-null="true" />
		</property>
		<property name="stg_level" type="java.lang.Short">
			<column name="stg_level" not-null="true" />
		</property>
		<property name="skill_index_1" type="java.lang.Integer">
			<column name="skill_index_1" not-null="true" />
		</property>
		<property name="skill_index_2" type="java.lang.Integer">
			<column name="skill_index_2" not-null="true" />
		</property>
		<property name="skill_index_3" type="java.lang.Integer">
			<column name="skill_index_3" not-null="true" />
		</property>
	</class>
</hibernate-mapping>