<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblNihongoProgressInfo" table="nihongo_progress_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="user_id" type="java.lang.Long">
			<column name="user_id" not-null="true" />
		</property>
		<property name="test_id" type="java.lang.Integer">
			<column name="test_id" not-null="true" />
		</property>
		<property name="progress" type="java.lang.Integer">
			<column name="progress" not-null="true" />
		</property>
		<property name="created" type="java.util.Date">
			<column name="created" not-null="true" />
		</property>
		<property name="updated" type="java.util.Date">
			<column name="updated" not-null="true" />
		</property>
	</class>
</hibernate-mapping>