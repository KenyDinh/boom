<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblSurveyOptionInfo" table="servey_option_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="survey_id" type="java.lang.Long">
			<column name="survey_id" not-null="true" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="name" length="255" not-null="true" />
		</property>
		<property name="content" type="java.lang.String">
			<column name="content" not-null="true" />
		</property>
		<property name="image" type="java.lang.String">
			<column name="image" not-null="true" />
		</property>
		<property name="video" type="java.lang.String">
			<column name="video" not-null="true" />
		</property>
		<property name="ref_url" type="java.lang.String">
			<column name="ref_url" length="255" not-null="true" />
		</property>
	</class>
</hibernate-mapping>