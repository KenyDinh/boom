<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblQuizInfo" table="quiz_info" catalog="boom">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="host" type="java.lang.Long">
            <column name="host" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="32" not-null="true" />
        </property>
        <property name="subject" type="java.lang.Byte">
            <column name="subject" not-null="true" />
        </property>
        <property name="level" type="java.lang.Byte">
            <column name="level" not-null="true" />
        </property>
         <property name="max_player" type="java.lang.Byte">
            <column name="max_player" not-null="true" />
        </property>
        <property name="question_num" type="java.lang.Byte">
            <column name="question_num" not-null="true" />
        </property>
        <property name="time_per_question" type="java.lang.Long">
            <column name="time_per_question" not-null="true" />
        </property>
        <property name="status" type="java.lang.Byte">
            <column name="status" not-null="true" />
        </property>
        <property name="retry" type="java.lang.Byte">
            <column name="retry" not-null="true" />
        </property>
        <property name="flag" type="java.lang.Byte">
            <column name="flag" not-null="true" />
        </property>
        <property name="player_num" type="java.lang.Byte">
            <column name="player_num" not-null="true" />
        </property>
        <property name="current_question" type="java.lang.Byte">
            <column name="current_question" not-null="true" />
        </property>
        <property name="current_option_order" type="java.lang.String">
            <column name="current_option_order" not-null="true" />
        </property>
        <property name="question_data" type="java.lang.String">
            <column name="question_data" not-null="true" />
        </property>
        <property name="created" type="java.util.Date">
            <column name="created" not-null="true" />
        </property>
        <property name="expired" type="java.util.Date">
            <column name="expired" not-null="true" />
        </property>
        <property name="updated" type="java.util.Date">
            <column name="updated" not-null="true" />
        </property>
    </class>
</hibernate-mapping>