<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblQuizLogInfo" table="quiz_log_info" catalog="boom">
        <id name="id" type="java.lang.Long">
            <column name="id" />
             <generator class="increment" />
        </id>
        <property name="quiz_id" type="java.lang.Long">
            <column name="quiz_id" />
        </property>
        <property name="question_index" type="java.lang.Byte">
            <column name="question_index" />
        </property>
         <property name="user_id" type="java.lang.Long">
            <column name="user_id" />
        </property>
         <property name="username" type="java.lang.String">
            <column name="username" not-null="true" />
        </property>
        <property name="correct_answer" type="java.lang.String">
            <column name="correct_answer" not-null="true" />
        </property>
        <property name="player_answer" type="java.lang.String">
            <column name="player_answer" not-null="true" />
        </property>
    </class>
</hibernate-mapping>