<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblSurveyUserAccessInfo" table="survey_user_access_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="survey_id" type="java.lang.Long">
			<column name="survey_id" not-null="true" />
		</property>
		<property name="user_code" type="java.lang.String">
			<column name="user_code" not-null="true" />
		</property>
		<property name="flag" type="java.lang.Integer">
			<column name="flag" not-null="true" />
		</property>
	</class>
</hibernate-mapping>