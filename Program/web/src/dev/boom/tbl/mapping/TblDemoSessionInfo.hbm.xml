<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblDemoSessionInfo" table="demo_session_info" catalog="boom">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="demo_time" type="java.util.Date">
            <column name="demo_time" not-null="true" />
        </property>
        <property name="demo_location" type="java.lang.String">
            <column name="demo_location" length="32" not-null="true" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" not-null="true" />
        </property>
    </class>
</hibernate-mapping>