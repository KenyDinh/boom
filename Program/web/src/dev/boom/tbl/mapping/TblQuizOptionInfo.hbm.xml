<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblQuizOptionInfo" table="quiz_option_info" catalog="boom">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="question_id" type="java.lang.Integer">
            <column name="question_id" not-null="true" />
        </property>
        <property name="option" type="java.lang.String">
            <column name="option" not-null="true" />
        </property>
        <property name="correct" type="java.lang.Byte">
            <column name="correct" not-null="true" />
        </property>
    </class>
</hibernate-mapping>