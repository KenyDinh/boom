<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblSurveyInfo" table="survey_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" not-null="true" />
		</property>
		<property name="pathname" type="java.lang.String">
			<column name="pathname" not-null="true" />
		</property>
		<property name="status" type="java.lang.Byte">
			<column name="status" not-null="true" />
		</property>
		<property name="type" type="java.lang.Byte">
			<column name="type" not-null="true" />
		</property>
		<property name="flag" type="java.lang.Integer">
			<column name="flag" not-null="true" />
		</property>
		<property name="description" type="java.lang.String">
			<column name="description" not-null="true" />
		</property>
		<property name="created" type="java.util.Date">
			<column name="created" not-null="true" />
		</property>
		<property name="expired" type="java.util.Date">
			<column name="expired" not-null="true" />
		</property>
		<property name="updated" type="java.util.Date">
			<column name="updated" not-null="true" />
		</property>
	</class>
</hibernate-mapping>