<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblQuizQuestionInfo" table="quiz_question_info" catalog="boom">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="subject" type="java.lang.Byte">
            <column name="subject" not-null="true" />
        </property>
        <property name="type" type="java.lang.Byte">
            <column name="type" not-null="true" />
        </property>
        <property name="level" type="java.lang.Short">
            <column name="level" not-null="true" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" not-null="true" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="description" not-null="true" />
        </property>
    </class>
</hibernate-mapping>