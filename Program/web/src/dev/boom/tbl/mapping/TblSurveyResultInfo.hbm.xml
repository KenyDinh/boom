<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblSurveyResultInfo" table="survey_result_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="survey_id" type="java.lang.Long">
			<column name="survey_id" not-null="true" />
		</property>
		<property name="user_id" type="java.lang.String">
			<column name="user_id" not-null="true" />
		</property>
		<property name="username" type="java.lang.String">
			<column name="username" not-null="true" />
		</property>
		<property name="department" type="java.lang.String">
			<column name="department" length="64" not-null="true" />
		</property>
		<property name="result" type="java.lang.String">
			<column name="result" not-null="true" />
		</property>
		<property name="progress" type="java.lang.Byte">
			<column name="progress" not-null="true" />
		</property>
		<property name="updated" type="java.util.Date">
			<column name="updated" not-null="true" />
		</property>
	</class>
</hibernate-mapping>