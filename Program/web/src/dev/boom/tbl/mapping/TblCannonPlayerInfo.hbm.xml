<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblCannonPlayerInfo" table="cannon_player_info" catalog="boom">
        <id name="user_id" type="java.lang.Long">
            <column name="user_id" not-null="true"/>
        </id>
        <property name="username" type="java.lang.String">
            <column name="username" length="32" not-null="true" />
        </property>
        <property name="score" type="java.lang.Integer">
            <column name="score" not-null="true" />
        </property>
        <property name="status" type="java.lang.Byte">
            <column name="status" not-null="true" />
        </property>
        <property name="created" type="java.util.Date">
            <column name="created" not-null="true" />
        </property>
        <property name="updated" type="java.util.Date">
            <column name="updated" not-null="true" />
        </property>
    </class>
</hibernate-mapping>