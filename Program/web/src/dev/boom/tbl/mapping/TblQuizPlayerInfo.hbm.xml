<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblQuizPlayerInfo" table="quiz_player_info" catalog="boom">
        <id name="user_id" type="java.lang.Long">
            <column name="user_id" />
        </id>
        <property name="username" type="java.lang.String">
            <column name="username" not-null="true" />
        </property>
        <property name="quiz_id" type="java.lang.Long">
            <column name="quiz_id" />
        </property>
        <property name="status" type="java.lang.Byte">
            <column name="status" not-null="true" />
        </property>
        <property name="answer" type="java.lang.String">
            <column name="answer" not-null="true" />
        </property>
        <property name="correct_count" type="java.lang.Byte">
            <column name="correct_count" not-null="true" />
        </property>
        <property name="correct_point" type="java.lang.Integer">
            <column name="correct_point" not-null="true" />
        </property>
        <property name="updated" type="java.util.Date">
            <column name="updated" not-null="true" />
        </property>
    </class>
</hibernate-mapping>