		<property name="reward_nyapo" type="java.lang.Byte">
			<column name="reward_nyapo" not-null="true" />
		</property>
		<property name="created" type="java.util.Date">
			<column name="created" not-null="true" />
		</property>
	</class>
</hibernate-mapping>