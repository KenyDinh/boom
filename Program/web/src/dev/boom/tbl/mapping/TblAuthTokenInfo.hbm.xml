<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblAuthTokenInfo" table="auth_token_info" catalog="boom">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="token" type="java.lang.String">
            <column name="token" not-null="true" />
        </property>
        <property name="validator" type="java.lang.String">
            <column name="validator" not-null="true" />
        </property>
        <property name="user_id" type="java.lang.Long">
            <column name="user_id" not-null="true" />
        </property>
        <property name="expired" type="java.util.Date">
			<column name="expired" not-null="true" />
		</property>
    </class>
</hibernate-mapping>