<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblCannonBlockInfo" table="cannon_block_info" catalog="boom">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="user_id" type="java.lang.Long">
            <column name="user_id" not-null="true" />
        </property>
        <property name="user_board" type="java.lang.String">
            <column name="user_board" not-null="true" />
        </property>
        <property name="bot_board" type="java.lang.String">
            <column name="bot_board" not-null="true" />
        </property>
        <property name="user_hp" type="java.lang.Integer">
            <column name="user_hp" not-null="true" />
        </property>
        <property name="bot_hp" type="java.lang.Integer">
            <column name="bot_hp" not-null="true" />
        </property>
        <property name="status" type="java.lang.Byte">
            <column name="status" not-null="true" />
        </property>
    </class>
</hibernate-mapping>