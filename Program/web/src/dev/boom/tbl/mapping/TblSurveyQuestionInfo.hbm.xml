<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblSurveyQuestionInfo" table="survey_question_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="survey_id" type="java.lang.Long">
			<column name="survey_id" not-null="true" />
		</property>
		<property name="idx" type="java.lang.Byte">
			<column name="idx" not-null="true" />
		</property>
		<property name="type" type="java.lang.Byte">
			<column name="type" not-null="true" />
		</property>
		<property name="title" type="java.lang.String">
			<column name="title" not-null="true" />
		</property>
		<property name="content" type="java.lang.String">
			<column name="content" not-null="true" />
		</property>
		<property name="description" type="java.lang.String">
			<column name="description" not-null="true" />
		</property>
		<property name="min_choice" type="java.lang.Integer">
			<column name="min_choice" not-null="true" />
		</property>
		<property name="max_choice" type="java.lang.Integer">
			<column name="max_choice" not-null="true" />
		</property>
		<property name="optional" type="java.lang.Byte">
			<column name="optional" not-null="true" />
		</property>
	</class>
</hibernate-mapping>