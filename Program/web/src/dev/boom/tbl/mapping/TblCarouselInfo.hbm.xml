<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblCarouselInfo" table="carousel_info" catalog="boom">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" not-null="true" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="description" not-null="true" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="url" length="255" not-null="true" />
        </property>
        <property name="local" type="java.lang.Byte">
            <column name="local" not-null="true" />
        </property>
        <property name="available" type="java.lang.Byte">
            <column name="available" not-null="true" />
        </property>
    </class>
</hibernate-mapping>