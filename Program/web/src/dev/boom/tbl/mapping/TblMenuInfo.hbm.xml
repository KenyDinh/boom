<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dev.boom.tbl.info.TblMenuInfo" table="menu_info" catalog="boom">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="255" not-null="true" />
		</property>
		<property name="shop_id" type="java.lang.Long">
			<column name="shop_id" not-null="true" />
		</property>
		<property name="sale" type="java.lang.Short">
			<column name="sale" not-null="true" />
		</property>
		<property name="code" type="java.lang.String">
			<column name="code" length="30" not-null="true" />
		</property>
		<property name="max_discount" type="java.lang.Long">
			<column name="max_discount" not-null="true" />
		</property>
		<property name="shipping_fee" type="java.lang.Long">
			<column name="shipping_fee" not-null="true" />
		</property>
		<property name="description" type="java.lang.String">
			<column name="description" not-null="true" />
		</property>
		<property name="status" type="java.lang.Byte">
			<column name="status" not-null="true" />
		</property>
		<property name="created" type="java.util.Date">
			<column name="created" not-null="true" />
		</property>
		<property name="expired" type="java.util.Date">
			<column name="expired" not-null="true" />
		</property>
		<property name="updated" type="java.util.Date">
			<column name="updated" not-null="true" />
		</property>
	</class>
</hibernate-mapping>