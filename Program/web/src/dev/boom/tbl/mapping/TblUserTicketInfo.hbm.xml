<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dev.boom.tbl.info.TblUserTicketInfo" table="user_ticket_info" catalog="boom">
        <id name="id" type="java.lang.Long">
            <column name="id" />
        </id>
        <property name="user_id" type="java.lang.Long">
            <column name="user_id" not-null="true" />
        </property>
        <property name="ticket_type" type="java.lang.Short">
            <column name="ticket_type" not-null="true" />
        </property>
        <property name="ticket_remain" type="java.lang.Short">
            <column name="ticket_remain" not-null="true" />
        </property>
        <property name="total_num" type="java.lang.Integer">
            <column name="total_num" not-null="true" />
        </property>
    </class>
</hibernate-mapping>